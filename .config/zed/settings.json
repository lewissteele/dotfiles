{
  "features": {
    "edit_prediction_provider": "none"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "buffer_font_family": "Hack",
  "buffer_font_size": 16,
  "max_tabs": 6,
  "tab_size": 2,
  "ui_font_family": "Hack",
  "ui_font_size": 18,
  "vim_mode": true
}
